{
  "name": "gulp-modulizr",
  "version": "1.0.0",
  "description": "a modulizr build that can be used by gulp",
  "main": "build/index.js",
  "dependencies": {
    "event-stream": "^3.3.0",
    "gulp-util": "^3.0.4",
    "modernizr": "^3.0.0-alpha.3"
  },
  "devDependencies": {
    "babel": "^5.0.10",
    "babel-runtime": "^5.0.11",
    "chai": "^2.2.0",
    "coveralls": "^2.11.2",
    "lab": "^5.5.1"
  },
  "scripts": {
    "compile": "babel src -d build -D",
    "postinstall": "npm run compile",
    "test": "lab -T babel.js -S -l",
    "coverage": "lab -T babel.js -S -l --coverage -r lcov | ./node_modules/.bin/coveralls",
    "hcoverage": "lab -T babel.js -S -l --coverage -r html >| coverage.html && open coverage.html"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/romeovs/gulp-modulizr.git"
  },
  "keywords": [
    "gulp",
    "modulizr"
  ],
  "author": "Romeo Van Snick <romeovs@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/romeovs/gulp-modulizr/issues"
  },
  "homepage": "https://github.com/romeovs/gulp-modulizr"
}
